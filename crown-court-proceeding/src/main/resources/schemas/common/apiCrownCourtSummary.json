{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "apiCrownCourtSummary.json",
  "type": "object",
  "description": "Crown Court Summary",
  "properties": {
    "repOrderDecision": {
      "type": "string",
      "title": "Rep Order decision"
    },
    "repId": {
      "type": "integer",
      "title": "Crown court rep id"
    },
    "repType": {
      "type": "string",
      "title": "crown court rep type"
    },
    "repOrderDate": {
      "type": "string",
      "title": "crown court Rep Order decision",
      "format": "date-time"
    },
    "withdrawalDate": {
      "type": "string",
      "title": "crown court withdrawal date",
      "format": "date-time"
    },
    "sentenceOrderDate": {
      "type": "string",
      "title": "crown court sentence order date",
      "format": "date-time"
    },
    "evidenceFeeLevel": {
      "type": "string",
      "description": "The evidence fee level"
    }
  },
  "additionalProperties": false,
  "required": ["repOrderDecision", "repId", "repType", "repOrderDate", "withdrawalDate"]
}
