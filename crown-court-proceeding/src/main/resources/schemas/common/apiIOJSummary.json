{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "IOJSummary.json",
  "type": "object",
  "description": "Summary of IOJ Data for an Application",
  "properties": {
    "decisionResult": {
      "type": "string",
      "description": "The appeal decision result"
    },
    "iojResult": {
      "type": "string",
      "description": "The initial Ioj Result"
    },
    "appealTypeCode": {
      "type": "string",
      "description": "The appeal type code"
    },
    "appealTypeDate": {
      "type": "string",
      "description": "The date the appeal type was set",
      "format": "date-time"
    }
  },
  "additionalProperties": false,
  "required": ["iojResult"]
}
